<Application
    x:Class="InteropTools.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:themes="using:InteropTools.Themes"
    xmlns:local="using:InteropTools">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Intense/Themes/Intense.xaml" />
                <ResourceDictionary Source="ms-appx:///Themes/Generic.xaml" />
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <themes:InteropToolsThemeResources/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
